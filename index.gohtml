<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <title>HttpAndTemplate</title>
    </head>
    <body>
        {{if .}}
            {{range $key, $value := .}}
                <p><strong>key: {{$key}}</strong></p>
                <ul>{{range $value}}
                    <li>value:{{.}}</li>
                {{end}}</ul>
            {{end}}
        {{end}}

        <form action="/?fname=Mel" id="simpleForm" method="POST">
            <input type="text" id="fname" name="fname"/>
            <input type="submit" name="sbutton">
        </form>

    </body>
</html>
